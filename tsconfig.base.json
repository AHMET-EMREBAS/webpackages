{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@webpackages/access-policy": ["libs/access-policy/src/index.ts"],
      "@webpackages/auth": ["libs/auth/src/index.ts"],
      "@webpackages/auth-client": ["libs/auth-client/src/index.ts"],
      "@webpackages/boot-nest": ["libs/boot-nest/src/index.ts"],
      "@webpackages/clients": ["libs/clients/src/index.ts"],
      "@webpackages/clients/custom-forms": [
        "libs/clients/custom-forms/src/index.ts"
      ],
      "@webpackages/clients/form-groups": [
        "libs/clients/form-groups/src/index.ts"
      ],
      "@webpackages/clients/forms": ["libs/clients/forms/src/index.ts"],
      "@webpackages/clients/modules": ["libs/clients/modules/src/index.ts"],
      "@webpackages/clients/ngrx": ["libs/clients/ngrx/src/index.ts"],
      "@webpackages/clients/routes": ["libs/clients/routes/src/index.ts"],
      "@webpackages/controllers": ["libs/controllers/src/index.ts"],
      "@webpackages/data": ["libs/data/src/index.ts"],
      "@webpackages/database": ["libs/database/src/index.ts"],
      "@webpackages/entities": ["libs/entities/src/index.ts"],
      "@webpackages/gen": ["libs/gen/src/index.ts"],
      "@webpackages/hash": ["libs/hash/src/index.ts"],
      "@webpackages/material": ["libs/material/src/index.ts"],
      "@webpackages/material/core": ["libs/material/core/src/index.ts"],
      "@webpackages/material/form": ["libs/material/form/src/index.ts"],
      "@webpackages/material/input": ["libs/material/input/src/index.ts"],
      "@webpackages/material/layout": ["libs/material/layout/src/index.ts"],
      "@webpackages/material/modules": ["libs/material/modules/src/index.ts"],
      "@webpackages/material/table": ["libs/material/table/src/index.ts"],
      "@webpackages/metadata": ["libs/metadata/src/index.ts"],
      "@webpackages/models": ["libs/models/src/index.ts"],
      "@webpackages/notification": ["libs/notification/src/index.ts"],
      "@webpackages/path": ["libs/path/src/index.ts"],
      "@webpackages/property": ["libs/property/src/index.ts"],
      "@webpackages/query": ["libs/query/src/index.ts"],
      "@webpackages/query-builder": ["libs/query-builder/src/index.ts"],
      "@webpackages/resources": ["libs/resources/src/index.ts"],
      "@webpackages/rest": ["libs/rest/src/index.ts"],
      "@webpackages/types": ["libs/types/src/index.ts"],
      "@webpackages/utils": ["libs/utils/src/index.ts"],
      "@webpackages/validation": ["libs/validation/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
