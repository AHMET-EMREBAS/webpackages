{
  "name": "material-form",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/material/form/src",
  "prefix": "wp",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/material/form/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/material/form/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/material/form/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/material/form/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },

    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/material/form/.storybook",
        "browserTarget": "material-form:build-storybook",
        "compodoc": true,
        "compodocArgs": [
          "-e",
          "json",
          "-d",
          "libs/material/form/.storybook",
          "-p",
          "libs/material/form/tsconfig.json"
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/libs/material-form-docs",
        "configDir": "libs/material/form/.storybook",
        "browserTarget": "material-form:build-storybook",
        "compodoc": true,
        "compodocArgs": [
          "-e",
          "json",
          "-d",
          "libs/material/form/.storybook",
          "-p",
          "libs/material/form/tsconfig.json"
        ]
      }
    }
  }
}
